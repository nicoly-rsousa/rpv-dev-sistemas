algoritmo "semnome"
// Função :
// Autor :
// Data : 20/08/2025
// Seção de Declarações
var
i,j, result, total :inteiro
inicio
// Seção de Comandos
result:=0
para i de 0 ate 15  faca
   result:=result*3
   se i=0 entao
      result:=1
   fimse
   se i=1  entao
      result:=3
   fimse
   Escreval("3 ^ ",i, " = ", result)
fimpara

fimalgoritmo
